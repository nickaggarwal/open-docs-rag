{
  "name": "open-docs-rag-widget",
  "version": "1.2.2",
  "description": "A customizable AI-powered documentation widget that can be easily integrated into any website or React application",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles.css",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles.css": "./dist/styles.css",
    "./styles": "./dist/styles-manual.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:bundle && npm run build:npm",
    "build:bundle": "webpack --mode=production",
    "build:npm": "rollup -c",
    "build:dev": "webpack --mode=development",
    "dev": "webpack serve --mode=development",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts,.js",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "publishPatch": "npm version patch && npm publish",
    "publishMinor": "npm version minor && npm publish",
    "publishMajor": "npm version major && npm publish"
  },
  "keywords": [
    "widget",
    "documentation",
    "ai",
    "rag",
    "chat",
    "support",
    "react",
    "typescript",
    "vanilla-js",
    "customer-support",
    "chatbot",
    "docs-assistant"
  ],
  "author": "Naveen Pandurangi",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.0",
    "rollup-plugin-dts": "^5.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "tslib": "^2.8.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickaggarwal/open-docs-rag.git"
  },
  "bugs": {
    "url": "https://github.com/nickaggarwal/open-docs-rag/issues"
  },
  "homepage": "https://github.com/nickaggarwal/open-docs-rag/tree/main/widget#readme",
  "engines": {
    "node": ">=14.0.0"
  }
}
